{
  "name": "afrexia-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:pbt": "vitest --run --grep 'Property:'",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:webhook": "node scripts/test-webhook.js",
    "test:kv": "node scripts/test-kv-connection.js",
    "lighthouse": "lhci autorun",
    "analyze": "ANALYZE=true npm run build",
    "verify:monitoring": "node scripts/verify-monitoring-analytics.js",
    "sanity:import-origins": "npx tsx scripts/import-origins.ts",
    "sanity:import-categories": "npx tsx scripts/import-categories.ts",
    "sanity:import-all": "npx tsx scripts/import-all-data.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@react-email/components": "^1.0.6",
    "@sanity/client": "^6.24.1",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.22.0",
    "@sentry/nextjs": "^8.46.0",
    "@vercel/analytics": "^1.4.1",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "fuse.js": "^7.0.0",
    "gsap": "^3.12.5",
    "lenis": "^1.1.17",
    "lucide-react": "^0.563.0",
    "mapbox-gl": "^3.8.0",
    "next": "^15.1.6",
    "next-sanity": "^11.6.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^5.2.5",
    "react-hook-form": "^7.54.2",
    "react-map-gl": "^7.1.7",
    "resend": "^4.0.1",
    "sanity": "^4.22.0",
    "tailwind-merge": "^2.6.0",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next/bundle-analyzer": "^16.1.4",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.4",
    "fast-check": "^3.22.0",
    "jest-axe": "^10.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  }
}
